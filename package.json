{
  "name": "s-tier",
  "version": "1.0.1",
  "description": "S-tier S-Expression serialization & deserialization for JavaScript powered by OCaml ğŸ«âœ¨",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build-ocaml": "eval $(opam env) && dune build",
    "build-typescript": "tsup",
    "build": "npm run build-ocaml && npm run build-typescript",
    "publish-patch": "npm run build && npm version patch && npm publish",
    "test": "bun test ./_build/default/dist/src/s-tier.test.ts",
    "lint": "tsc"
  },
  "devDependencies": {
    "bun-types": "1.0.20",
    "fast-check": "^3.15.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "melange-ffi": "latest"
  }
}
